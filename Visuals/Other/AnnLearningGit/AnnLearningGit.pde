/* Working through Shiffman's "Learning Processing" 
This is my Zoog */
void setup(){
size(400,400);
}
void draw(){
background(255);
ellipseMode(CENTER);
rectMode(CENTER);
stroke(50);
fill(250,0,250);
rect(200,100,100,100);
fill(0,250,250);
ellipse(200,225,150,150);
fill(0);
ellipse(180, 100, 10,10);
ellipse(220, 100, 10,10);
fill(250);
ellipse(200, 120, 15,15);}